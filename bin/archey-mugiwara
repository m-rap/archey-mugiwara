#!/bin/bash

white='\e[1;37m'
yellow='\e[1;33m'
red='\e[1;31m'

echo -e "${white}
             .###.                                          .###
             #####                                          ####:
         .#######'                                          '#######
         #########.                 ${yellow}......${white}                 .########:
         '####'#####.          ${yellow}.#############.${white}           .####'''##'
                '#####.     ${yellow}.:##################.${white}      .####:'
                  '#####.  ${yellow}.######################.${white}  .#####'
                    '#### ${yellow}#######*************######.${white}'###'
                      '# ${red}...ooooo#############ooooo..${white} #'
                        ${red}#'''${yellow}...oooooooooooooooo....${red}'''
                 ${yellow}...oo####***''''''''''''''''*******#####o:..
             ${yellow}.o#######'${white}.o###########################oo.${yellow}'#######o.
                  '''''${white}#####*'   '*########'     '#####${yellow} '''
                       ${white}:###         '#####         '###
                       '##           :###           ##:
                        '#           :###           ##
                         'o         .#####         .#'
                          'o..    .##'  '##..    .#*
                            '#######:    '#######*'
                            .o'*#####o  .o####*'..
                          .###.#o.'.'****'''.o#.:#o.
                        .####:'##::####:####'##''####.
                      .#####' ##. .''**'***' .o# '#####.
                    .#####'   *## ####:o####.##''  '#####
                  .#####'     #o...''*''**'..oo#:    '####o
                .#####'       *#################       '####o
          ... .#####'          '###############'         '####o ...
         :########*              '***#######'              *########:
         '######:                                           '#######'
          '' :###o                                          ####: '
             :####                                          *###'
              ''                                              '
"
